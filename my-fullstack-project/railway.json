{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfile": "my-fullstack-project/backend/Dockerfile",
    "context": "my-fullstack-project"
  },
  "deploy": {
    "restartPolicyType": "on_failure",
    "restartPolicyMaxRetries": 5,
  "startCommand": "cd my-fullstack-project && ./start.sh",
    "numReplicas": 1
  }
}
